<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="org.addondev.ui.editor.javascript.texthover" name="javascript.texthover" schema="schema/org.addondev.ui.editor.javascript.texthover.exsd"/>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.addondev.ui.editor.javascript.JavaScriptEditor"
            default="false"
            extensions="js"
            id="org.addondev.ui.editor.javascript"
            name="AddonDev JavaScript Editor">
      </editor>
      <editor
            class="org.addondev.ui.editor.xul.XULEditor"
            default="false"
            extensions="xul"
            id="org.addondev.editor.xul"
            name="AddonDev XUL Editor">
      </editor>
      <editor
            class="org.addondev.ui.editor.xul.preview.XULPreviewForm"
            default="false"
            extensions="xul"
            id="org.addondev.editor.xul.formeditor"
            name="AddonDev XUL Preview">
      </editor>
      <editor
            class="org.addondev.ui.editor.xulmultipageeditor.XULMultiPageEditor"
            default="false"
            extensions="xul"
            id="org.addondev.ui.editor.xulmultipageeditor.XULMultiPageEditor"
            name="XUL">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.editorActions">
      <editorContribution
            id="org.addondev.ui.SelectLineRulerActions"
            targetID="org.addondev.ui.editor.javascript">
         <action
               actionID="RulerClick"
               class="org.addondev.ui.actions.SelectLineRulerActionDelegate"
               id="org.addondev.debug.SelectLineRulerAction"
               label="SelectLine">
         </action>
      </editorContribution>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="org.addondev.category"
            name="Addon Dev">
      </category>
      <wizard
            category="org.addondev.category"
            class="org.addondev.ui.wizard.AddonDevNewProjectWizard"
            hasPages="true"
            icon="icons/addon.png"
            id="org.addondev.wizzard.addondevwizard"
            name="AddonDev Project"
            project="true">
      </wizard>
      <wizard
            category="org.addondev.category"
            class="org.addondev.ui.wizard.NewJavaScriptFileWizard"
            id="org.addondev.wizard1"
            name="JavaScript File"
            project="false">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.importWizards">
      <wizard
            category="org.addondev.ui.wizard.import"
            class="org.addondev.ui.wizard.AddonDevImportWizard"
            id="org.addondev.wizard.addonimportwizard"
            name="Firefox Addon">
      </wizard>
      <category
            id="org.addondev.ui.wizard.import"
            name="Addon Dev">
      </category>
   </extension>
   <extension
         point="org.addondev.builder">
      <new_element
            class="org.addondev.ui.builder.AddonDevBuilder">
      </new_element>
   </extension>
   <extension
         point="org.eclipse.ui.editors.templates">
      <contextType
            class="org.addondev.ui.template.JavaScriptTemplateContextType"
            id="org.addondev.ui.template.javascript"
            name="JavaScript">
      </contextType>
      <include
            file="templates/code/javascript_templates.xml">
      </include>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="AddonDev.category1"
            name="name">
      </category>
      <command
            categoryId="AddonDev.category1"
            id="AddonDev.command1"
            name="name cmd">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.addondev.ui.editor.javascript.actions.samplehandler"
            commandId="AddonDev.command1">
      </handler>
      <handler
            class="org.addondev.ui.editor.xulmultipageeditor.actions.RefreshHandler"
            commandId="org.addondev.ui.handler1">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="AddonDev.command1"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+6">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:edit?after=additions">
         <command
               commandId="AddonDev.command1"
               label="test"
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.addondev.ui.preferencepages"
            class="org.addondev.ui.preferences.AddonDevNewProjectPreferencePage"
            id="org.addondev.ui.preferencepages"
            name="AddonDev">
      </page>
      <page
            category="org.addondev.ui.preferencepages"
            class="org.addondev.ui.preferences.XULEditorPreferencePage"
            id="org.addondev.ui.XULPreferencepages"
            name="XUL Editor">
      </page>
      <page
            category="org.addondev.ui.preferencepages"
            class="org.addondev.ui.preferences.JavaScriptEditorPreferencePage"
            id="AddonDev.page1"
            name="name">
      </page>
      <page
            category="org.addondev.ui.preferencepages"
            class="org.addondev.ui.preferences.JavaScriptTemplatePreferencePage"
            id="org.addondev.ui.page1"
            name="name">
      </page>
      <page
            class="org.addondev.ui.WorkbenchPreferencePage1"
            id="org.addondev.ui.page1"
            name="name">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.addondev.ui.preferences.AddonDevUIPreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.propertyPages">
      <page
            class="org.addondev.ui.propertypage.AddonDevPropertyPage"
            id="org.addondev.ui.propertypage.AddonDevPropertyPage"
            name="name">
         <enabledWhen>
            <instanceof
                  value="org.eclipse.core.resources.IProject">
            </instanceof>
         </enabledWhen>
      </page>
   </extension>
   <extension
         point="org.eclipse.ui.editors.templates">
      <contextType
            class="org.addondev.ui.template.ExtensionTemplateContextType"
            id="org.addondev.ui.template.Extension">
      </contextType>
      <template
            contextTypeId="org.addondev.ui.template.Extension"
            id="org.addondev.ui.template6"
            name="contextmenu">
         <pattern>
            &lt;popup id=&quot;contentAreaContextMenu&quot;&gt;
    &lt;menuitem id=&quot;${name}-context&quot; label=&quot;&amp;${name}.context.label;&quot;
              accesskey=&quot;&amp;${name}.context.accesskey;&quot;
              insertafter=&quot;context-stop&quot;
              oncommand=&quot;&quot;/&gt;
  &lt;/popup&gt;
         </pattern>
      </template>
      <template
            autoinsert="true"
            contextTypeId="org.addondev.ui.template.Extension"
            id="org.addondev.ui.template7"
            name="menu_template">
         <pattern>
            &lt;menupopup id=&quot;menu_ToolsPopup&quot;&gt;
    &lt;menuitem id=&quot;${name}-menu&quot; label=&quot;&amp;${name}.menu.label;&quot;
              oncommand=&quot;&quot;/&gt;
&lt;/menupopup&gt;
         </pattern>
      </template>
      <template
            autoinsert="true"
            contextTypeId="org.addondev.ui.template.Extension"
            id="org.addondev.ui.template8"
            name="sidebar">
         <pattern>
            &lt;menupopup id=&quot;viewSidebarMenu&quot;&gt;
    &lt;menuitem key=&quot;key_openSidebar_sidebar&quot; observes=&quot;viewSidebar_sidebar&quot; /&gt;
  &lt;/menupopup&gt;

  &lt;keyset id=&quot;mainKeyset&quot;&gt;
    &lt;key id=&quot;key_openSidebar_sidebar&quot; command=&quot;viewSidebar_sidebar&quot; /&gt;
  &lt;/keyset&gt;

  &lt;broadcasterset id=&quot;mainBroadcasterSet&quot;&gt;
    &lt;broadcaster id=&quot;viewSidebar_sidebar&quot;
                 label=&quot;&amp;${name}.sidebar.label;&quot;
                 autoCheck=&quot;false&quot;
                 type=&quot;checkbox&quot;
                 group=&quot;sidebar&quot;
                 sidebarurl=&quot;chrome://${name}/content/${name}-sidebar.xul&quot;
                 sidebartitle=&quot;&amp;${name}.sidebar.label;&quot;
                 oncommand=&quot;&quot; /&gt;
  &lt;/broadcasterset&gt;
         </pattern>
      </template>
                 
                  <template
                        autoinsert="true"
                        contextTypeId="org.addondev.ui.template.Extension"
                        id="org.addondev.ui.template9"
                        name="toolbar">
                     <pattern>
                        &lt;toolbarpalette id=&quot;BrowserToolbarPalette&quot;&gt;
  &lt;toolbarbutton id=&quot;toobarbutton-toolbar-button&quot; class=&quot;toolbarbutton-1 chromeclass-toolbar-additional&quot;
    label=&quot;&amp;toobarbuttonToolbarButton.label;&quot; tooltiptext=&quot;&amp;toobarbuttonToolbarButton.tooltip;&quot;
    oncommand=&quot;toobarbutton.onToolbarButtonCommand()&quot;/&gt;
  &lt;/toolbarpalette&gt;
                     </pattern>
                  </template> <template
                        autoinsert="true"
                        contextTypeId="org.addondev.ui.template.Extension"
                        id="org.addondev.ui.template9"
                        name="toolbarbutton">
                     <pattern>
                        &lt;toolbarpalette id=&quot;BrowserToolbarPalette&quot;&gt;
  &lt;toolbarbutton id=&quot;toobarbutton-toolbar-button&quot; class=&quot;toolbarbutton-1 chromeclass-toolbar-additional&quot;
    label=&quot;&amp;toobarbuttonToolbarButton.label;&quot; tooltiptext=&quot;&amp;toobarbuttonToolbarButton.tooltip;&quot;
    oncommand=&quot;toobarbutton.onToolbarButtonCommand()&quot;/&gt;
  &lt;/toolbarpalette&gt;

                     </pattern>
                  </template>
                              <template
                                    autoinsert="true"
                                    contextTypeId="org.addondev.ui.template.Extension"
                                    id="org.addondev.ui.template2"
                                    name="overlay">
                                 <pattern>
                                    &lt;?xml version="1.0" encoding="UTF-8"?&gt;
&lt;?xml-stylesheet href="chrome://global/skin/global.css" type="text/css"?&gt;
&lt;?xml-stylesheet href="chrome://${name}/skin/${name}.css" type="text/css"?&gt;
&lt;!DOCTYPE overlay SYSTEM "chrome://${name}/locale//${name}.dtd"&gt;

&lt;overlay id="${name}-overlay" xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul"&gt;
  &lt;script type="application/x-javascript;version=${javascriptvar}" src="chrome://${name}/content/${name}.js" /&gt;

  &lt;stringbundleset id="stringbundleset"&gt;
    &lt;stringbundle id="context-strings" src="chrome://${name}/locale/${name}.properties"/&gt;
  &lt;/stringbundleset&gt;

  ${context_template}
  ${menu_template}
  ${sidebar_template}
  ${toolbarbutton_template}
  ${toolbar_template}

&lt;/overlay&gt;
                                 </pattern>
                              </template>
                              <template
                                    autoinsert="true"
                                    contextTypeId="org.addondev.ui.template.Extension"
                                    id="org.addondev.ui.template1"
                                    name="install.rfd">
                                 <pattern>
                                    &lt;?xml version=&quot;1.0&quot; encoding=&quot;UTF-8&quot;?&gt;
&lt;RDF xmlns=&quot;http://www.w3.org/1999/02/22-rdf-syntax-ns#&quot; xmlns:em=&quot;http://www.mozilla.org/2004/em-rdf#&quot;&gt;
  &lt;Description about=&quot;urn:mozilla:install-manifest&quot;&gt;
    &lt;em:id&gt;${id}&lt;/em:id&gt;
    &lt;em:type&gt;2&lt;/em:type&gt;
    &lt;em:name&gt;${name}&lt;/em:name&gt;
    &lt;em:version&gt;${version}&lt;/em:version&gt;
    &lt;em:creator&gt;${creator}&lt;/em:creator&gt;
    &lt;em:optionsURL&gt;${options}&lt;/em:optionsURL&gt;
    &lt;em:targetApplication&gt;
      &lt;Description&gt;
        &lt;em:id&gt;${appid}&lt;/em:id&gt;
        &lt;em:minVersion&gt;${minversion}&lt;/em:minVersion&gt;
        &lt;em:maxVersion&gt;${maxversion}&lt;/em:maxVersion&gt;
      &lt;/Description&gt;
    &lt;/em:targetApplication&gt;
  &lt;/Description&gt;
&lt;/RDF&gt;
                                 </pattern>
                              </template>
                              <template
                                    autoinsert="true"
                                    contextTypeId="org.addondev.ui.template2"
                                    id="org.addondev.ui.template2"
                                    name="chrome.manifest">
                                 <pattern>
                                    content ${name} chrome/content/
skin    ${name} classic/1.0 chrome/skin/classic/
overlay chrome://browser/content/browser.xul chrome://${name}/content/overlay.xul
${locates}
                                 </pattern>
                              </template>
   </extension>

</plugin>
