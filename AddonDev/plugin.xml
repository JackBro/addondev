<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.editorActions">
      <editorContribution
            id="jp.addondev.internal.debug.BreakpointRulerActions"
            targetID="jp.addondev.editor.javascripteditor">
         <action
               actionID="RulerDoubleClick"
               class="jp.addondev.debug.ui.actions.JSManageBreakpointRulerActionDelegate"
               id="jp.addondev.internal.debug.BreakpointRulerAction"
               label="ToggleBreakPoint">
         </action>
      </editorContribution>
      <editorContribution
            id="jp.addondev.internal.debug.ui.actions.BreakpointRulerActions"
            targetID="jp.addondev.editor.javascripteditor">
         <action
               actionID="RulerDoubleClick"
               class="jp.addondev.debug.ui.actions.JSManageBreakpointRulerActionDelegate"
               id="jp.addondev.internal.debug.ui.actions.ManageBreakpointRulerAction"
               label="ToggleBreakPoint">
         </action>
      </editorContribution>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTabGroups">
      <launchConfigurationTabGroup
            class="jp.addondev.debug.ui.launching.AddonDevTabGroup"
            id="jp.addondev.debug.ui.launchConfigurationTabGroup.js"
            type="jp.addondev.debug.core.launchConfigurationType">
      </launchConfigurationTabGroup>
   </extension>
   <extension
         point="org.eclipse.debug.core.launchConfigurationTypes">
      <launchConfigurationType
            delegate="jp.addondev.debug.core.launching.AddonDevLaunchDelegate"
            id="jp.addondev.debug.core.launchConfigurationType"
            modes="run, debug"
            name="AddonDev"
            public="true"
            sourceLocatorId="org.eclipse.jdt.launching.sourceLocator.JavaSourceLookupDirector"
            sourcePathComputerId="org.eclipse.jdt.launching.sourceLookup.javaSourcePathComputer">
      </launchConfigurationType>
   </extension>
   <extension
         point="org.eclipse.debug.core.breakpoints">
      <breakpoint
            class="jp.addondev.debug.core.model.JSLineBreakpoint"
            id="addonStopBreakpoint"
            markerType="AddonDev.addonStopBreakpointMarker">
      </breakpoint>
   </extension>
   <extension
         point="org.eclipse.debug.ui.debugModelPresentations">
      <debugModelPresentation
            class="jp.addondev.debug.ui.model.JSDebugModelPresentation"
            id="jp.addondev.debug">
      </debugModelPresentation>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchShortcuts">
      <shortcut
            class="jp.addondev.debug.ui.launching.LaunchShortcut"
            icon="icons/addon.png"
            id="jp.javascript.debug.ui.launchShortcut.js"
            label="Addon Run"
            modes="run, debug">
         <perspective
               id="org.eclipse.debug.ui.DebugPerspective">
         </perspective>
         <configurationType
               id="jp.addondev.debug.core.launchConfigurationType">
         </configurationType>
         <contextualLaunch>
            <enablement>
               <with
                     variable="selection">
                  <count
                        value="1">
                  </count>
                  <iterate>
                     <or>
                        <test
                              property="org.eclipse.debug.ui.matchesPattern"
                              value="*.js">
                        </test>
                        <test
                              property="org.eclipse.debug.ui.matchesPattern"
                              value="*.xul">
                        </test>
                     </or>
                  </iterate>
               </with>
            </enablement>
         </contextualLaunch>
      </shortcut>
      <shortcut
            class="jp.addondev.debug.ui.launching.LaunchShortcut"
            id="jp.javascript.debug.ui.launchShortcut.greasemonkey"
            label="Greasemonkey"
            modes="debug">
         <perspective
               id="org.eclipse.debug.ui.DebugPerspective">
         </perspective>
         <configurationType
               id="jp.addondev.debug.core.launchConfigurationType.greasemonkey"></configurationType>
         <contextualLaunch>
            <enablement>
               <with
                     variable="selection">
                  <count
                        value="1">
                  </count>
                  <iterate>
                     <or>
                        <test
                              property="org.eclipse.debug.ui.matchesPattern"
                              value="*.js">
                        </test>
                        <test
                              property="org.eclipse.debug.ui.matchesPattern"
                              value="*.xul">
                        </test>
                     </or>
                  </iterate>
               </with>
            </enablement>
         </contextualLaunch>
      </shortcut>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="jp.javascript.debug"
            name="JavaScriptDev">
      </category>
      <view
            class="jp.addondev.debug.ui.PseudoSplitView"
            id="jp.addondev.debug.ui.PseudoSplitView"
            name="name"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="jp.addondev.preferences.AddonDevPreferencePage"
            id="jp.addondev.debug.preferencepages"
            name="AddonDev">
      </page>
      <page
            category="jp.addondev.debug.preferencepages"
            class="jp.addondev.preferences.JavaScriptEditorPreferencePage"
            id="AddonDev.page1"
            name="name">
      </page>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="jp.addondev.editor.javascript.JavaScriptEditor"
            default="false"
            extensions="js"
            id="jp.addondev.editor.javascripteditor"
            name="AddonDev JavaScript Editor">
      </editor>
      <editor
            class="jp.addondev.editor.xul.XULEditor"
            default="false"
            extensions="xul"
            id="jp.addondev.editor.xul"
            name="AddonDev XUL Editor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="jp.addondev.category.wizard.new"
            name="Addon Dev">
      </category>
      <wizard
            category="jp.addondev.category.wizard.new"
            class="jp.addondev.wizard.AddonDevWizard"
            hasPages="true"
            icon="icons/addon.png"
            id="jp.addondev.wizzard.addondevwizard"
            name="AddonDev Project"
            project="true">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.importWizards">
      <wizard
            category="jp.addondev.category.wizard.import"
            class="jp.addondev.wizard.AddonDevImportWizard"
            id="jp.addondev.wizard.addonimportwizard"
            name="Firefox Addon">
      </wizard>
      <category
            id="jp.addondev.category.wizard.import"
            name="Addon Dev">
      </category>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTypeImages">
      <launchConfigurationTypeImage
            configTypeID="jp.addondev.debug.core.launchConfigurationType"
            icon="icons/addon.png"
            id="jp.addondev.debug.ui.launchConfigurationTypeImage">
      </launchConfigurationTypeImage>
   </extension>
   <extension
         point="org.eclipse.ui.ide.markerImageProviders">
      <imageprovider
            class="jp.addondev.debug.core.model.JSLineBreakpoint"
            icon="icons/bp_enable.png"
            id="addonBreakPointImage"
            markertype="AddonDev.addonStopBreakpointMarker">
      </imageprovider>
   </extension>
   <extension
         id="addonStopBreakpointMarker"
         point="org.eclipse.core.resources.markers">
      <super
            type="org.eclipse.debug.core.lineBreakpointMarker">
      </super>
      <persistent
            value="true">
      </persistent>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="AddonDev.category1"
            name="name">
      </category>
      <command
            categoryId="AddonDev.category1"
            id="AddonDev.command1"
            name="name cmd">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="jp.addondev.editor.javascript.actions.samplehandler"
            commandId="AddonDev.command1">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:edit?after=additions">
         <command
               commandId="AddonDev.command1"
               label="test"
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.ide.projectNatureImages">
      <image
            icon="icons/addondev_nature.png"
            id="AddonDev.image1"
            natureId="AddonDev.addondevnature">
      </image>
   </extension>
   <extension
         id="addondevnature"
         name="addondevNature"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="jp.addondev.core.AddonDevNature">
         </run>
      </runtime>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="AddonDev.command1"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+6">
      </key>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="jp.addondev.preferences.AddonDevPreferenceInitializer">
      </initializer>
   </extension>

</plugin>
